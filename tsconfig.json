      "@acpaas-ui/ngx-localstorage": [
        "packages/ngx-localstorage/dist"
      ],
      "@acpaas-ui/ngx-localstorage/*": [
        "packages/ngx-localstorage/dist/*"
      ],
      "@acpaas-ui/ngx-pagination": [
        "packages/ngx-pagination/dist"
      ],
      "@acpaas-ui/ngx-pagination/*": [
        "packages/ngx-pagination/dist/*"
      ],
      "@acpaas-ui/ngx-progress-bar": [
        "packages/ngx-progress-bar/dist"
      ],
      "@acpaas-ui/ngx-progress-bar/*": [
        "packages/ngx-progress-bar/dist/*"
      ],
      "@acpaas-ui/ngx-table": [
