{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@acpaas-ui/ngx-layout": [
        "packages/ngx-layout/dist"
      ],
      "@acpaas-ui/ngx-layout/*": [
        "packages/ngx-layout/dist/*"
      ],
      "@acpaas-ui/ngx-logo": [
        "packages/ngx-logo/dist"
      ],
