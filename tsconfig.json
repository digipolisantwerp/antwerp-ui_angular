        "packages/ngx-agenda/dist/*"
      ],
      "@acpaas-ui/ngx-avatar": [
        "packages/ngx-avatar/dist"
      ],
      "@acpaas-ui/ngx-avatar/*": [
        "packages/ngx-avatar/dist/*"
      ],
      "@acpaas-ui/ngx-calendar": [
        "packages/ngx-calendar/dist"
      ],
      "@acpaas-ui/ngx-calendar/*": [
        "packages/ngx-calendar/dist/*"
