        "packages/ngx-logo/dist/*"
      ],
      "@acpaas-ui/ngx-navigation-menu": [
        "packages/ngx-navigation-menu/dist"
      ],
      "@acpaas-ui/ngx-navigation-menu/*": [
        "packages/ngx-navigation-menu/dist/*"
