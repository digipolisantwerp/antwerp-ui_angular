        "packages/ngx-selectable-list/dist/*"
      ],
      "@acpaas-ui/ngx-forms": [
        "packages/ngx-forms/dist"
      ],
      "@acpaas-ui/ngx-forms/*": [
        "packages/ngx-forms/dist/*"
      ],
      "@acpaas-ui/ngx-utils": [
        "packages/ngx-utils/dist"
      ],
      "@acpaas-ui/ngx-utils/*": [
        "packages/ngx-utils/dist/*"
      ],
      "@acpaas-ui/ngx-leaflet": [
